<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="SingleWordTermGatherer">
      <import name="eu.project.ttc.all.engines.indexer.SingleWordTermGatherer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishMultiWordTermGatherer">
      <import name="eu.project.ttc.english.engines.indexer.EnglishMultiWordTermGatherer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>English Term Gatherer</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>EditDistanceClassName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>SingleWordTermGatherer/EditDistanceClassName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Enable</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>SingleWordTermGatherer/Enable</parameter>
          <parameter>EnglishMultiWordTermGatherer/Enable</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Threshold</name>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>SingleWordTermGatherer/Threshold</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Ngrams</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>SingleWordTermGatherer/Ngrams</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>EditDistanceClassName</name>
        <value>
          <string>eu.project.ttc.metrics.LevenshteinDistance</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Enable</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Threshold</name>
        <value>
          <float>0.9</float>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Ngrams</name>
        <value>
          <integer>1</integer>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>SingleWordTermGatherer</node>
        <node>EnglishMultiWordTermGatherer</node>
      </fixedFlow>
    </flowConstraints>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
