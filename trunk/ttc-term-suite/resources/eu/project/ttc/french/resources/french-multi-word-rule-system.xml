	<pattern id="noun-preposition-noun">
		<sequence>
			<patterns>
				<pattern>
					<annotation type="eu.project.ttc.types.WordAnnotation">
						<constraint>
							<eq key="category" value="noun" />
						</constraint>
					</annotation>
				</pattern>
				<pattern>
					<annotation type="eu.project.ttc.types.WordAnnotation">
						<constraint>
							<and>
								<constraints>
									<constraint>
										<eq key="category" value="adposition" />
									</constraint>
									<constraint>
										<eq key="subCategory" value="preposition" />
									</constraint>
								</constraints>
							</and>
						</constraint>
					</annotation>
				</pattern>
				<!-- TODO option determiner for some preposition "dans sous avant après 
					chez " + det déf "sur" + dét déf? "en pour par" no dét -->
				<pattern>
					<option>
						<pattern>
							<annotation type="eu.project.ttc.types.WordAnnotation">
								<constraint>
									<or>
									<constraints>
									<constraint>
										<eq key="category" value="determiner" />
										</constraint>
										<constraint>
										<eq key="category" value="article" />
										</constraint>
									</constraints>
									</or>
								</constraint>
							</annotation>
						</pattern>
					</option>
				</pattern>
				<pattern>
					<annotation type="eu.project.ttc.types.WordAnnotation">
						<constraint>
							<and>
								<constraints>
									<constraint>
										<eq key="category" value="noun" />
									</constraint>
									<constraint>
										<neq key="lemma" value="axe" />
									</constraint>
									<constraint>
										<neq key="lemma" value="calage" />
									</constraint>
									<constraint>
										<neq key="lemma" value="chair" />
									</constraint>
									<constraint>
										<neq key="lemma" value="couleur" />
									</constraint>
									<constraint>
										<neq key="lemma" value="effet" />
									</constraint>
									<constraint>
										<neq key="lemma" value="état" />
									</constraint>
									<constraint>
										<neq key="lemma" value="face" />
									</constraint>
									<constraint>
										<neq key="lemma" value="genre" />
									</constraint>
									<constraint>
										<neq key="lemma" value="origine" />
									</constraint>
									<constraint>
										<neq key="lemma" value="pas" />
									</constraint>
									<constraint>
										<neq key="lemma" value="pâte" />
									</constraint>
									<constraint>
										<neq key="lemma" value="phase" />
									</constraint>
									<constraint>
										<neq key="lemma" value="type" />
									</constraint>
									<constraint>
										<neq key="lemma" value="vitesse" />
									</constraint>
									<constraint>
										<neq key="lemma" value="voie" />
									</constraint>
								</constraints>
							</and>
						</constraint>
					</annotation>
				</pattern>
			</patterns>
		</sequence>
	</pattern>
