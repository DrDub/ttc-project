<patterns>
	<pattern id="noun-noun">
		<sequence>
			<patterns>
				<pattern>
					<annotation type="eu.project.ttc.types.WordAnnotation">
						<constraint>
							<eq key="category" value="noun" />
						</constraint>
					</annotation>
				</pattern>
				<pattern>
					<annotation type="eu.project.ttc.types.WordAnnotation">
						<constraint>
							<or>
							<constraints>
								<constraint>
								<eq key="category" value="noun" />
								</constraint>
								<constraint>
								<eq key="category" value="name" />
								</constraint>
								</constraints>
							</or>
						</constraint>
					</annotation>
				</pattern>
			</patterns>
		</sequence>
	</pattern>
	<pattern id="noun-adjective">
		<sequence>
			<patterns>
				<pattern>
					<annotation type="eu.project.ttc.types.WordAnnotation">
						<constraint>
							<eq key="category" value="noun" />
						</constraint>
					</annotation>
				</pattern>
				<pattern>
					<annotation type="eu.project.ttc.types.WordAnnotation">
					  <constraint>
					    <or>
					      <constraints>
						<constraint>
							<eq key="category" value="adjective" />
						</constraint>
						<constraint>
						  <and>
								<constraints>
									<constraint>
										<eq key="category" value="verb" />
									</constraint>
									<constraint>
										<eq key="mood" value="participle" />
									</constraint>
									<constraint>
										<eq key="tense" value="past" />
									</constraint>
								</constraints>
						  </and>
						</constraint>
					      </constraints>
					    </or>					      
					  </constraint>
					</annotation>
				</pattern>
			</patterns>
		</sequence>
	</pattern>
