<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="EnglishTermGatherer">
      <import name="eu.project.ttc.english.engines.EnglishTermGatherer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTermFilter">
      <import name="eu.project.ttc.english.engines.EnglishTermFilter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishCompoundSplitter">
      <import name="eu.project.ttc.english.engines.EnglishCompoundSplitter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="IndexWriter">
      <import name="eu.project.ttc.all.engines.IndexWriter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="XmiCasConsumer">
      <import name="eu.project.ttc.all.engines.XmiCasConsumer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishLemmaFilter">
      <import name="eu.project.ttc.english.engines.EnglishLemmaFilter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTermIndexer">
      <import name="eu.project.ttc.english.engines.EnglishTermIndexer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermCleaner">
      <import name="eu.project.ttc.all.engines.TermCleaner"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTermDetector">
      <import name="eu.project.ttc.english.engines.EnglishTermDetector"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermIndexer">
      <import name="eu.project.ttc.all.engines.TermIndexer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>English Acabit Analysis Engine</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>Directory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>XmiCasConsumer/Directory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>File</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>IndexWriter/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Type</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermIndexer/Type</parameter>
          <parameter>TermCleaner/Type</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Language</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermIndexer/Language</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TermVariationRuleFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTermGatherer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>MultiWordPatternRuleFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTermDetector/MultiWordPatternRuleFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>NeoclassicalElementFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTermDetector/NeoclassicalElementFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Dictionary</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishCompoundSplitter/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>EditDistanceClassName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>EnglishTermGatherer/EditDistanceClassName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>EnableTermGathering</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>EnglishTermGatherer/Enable</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>Directory</name>
        <value>
          <string>/tmp</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>File</name>
        <value>
          <string>en-terminology.xmi</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Type</name>
        <value>
          <string>eu.project.ttc.types.TermAnnotation</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Language</name>
        <value>
          <string>en</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>EditDistanceClassName</name>
        <value>
          <string>eu.project.ttc.metrics.LevenshteinDistance</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>EnableTermGathering</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>EnglishTermDetector</node>
        <node>EnglishTermFilter</node>
        <node>EnglishLemmaFilter</node>
        <node>TermIndexer</node>
        <node>EnglishTermIndexer</node>
        <node>EnglishCompoundSplitter</node>
        <node>EnglishTermGatherer</node>
        <node>TermCleaner</node>
        <node>IndexWriter</node>
        <node>XmiCasConsumer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.SourceDocumentInformation</type>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </inputs>
        <outputs/>
        <languagesSupported>
          <language>en</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>DefaultIndex</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/all/engines/IndexWriter.xml</fileUrl>
        </fileResourceSpecifier>
        <implementationName>uima.sandbox.indexer.resources.DefaultIndex</implementationName>
      </externalResource>
      <externalResource>
        <name>EnglishFilter</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/english/resources/english-stop-word-filter.xml</fileUrl>
        </fileResourceSpecifier>
        <implementationName>uima.sandbox.filter.resources.FilterResource</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>EnglishTermGatherer/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>EnglishTermIndexer/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermIndexer/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>IndexWriter/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>EnglishTermFilter/Filter</key>
        <resourceName>EnglishFilter</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>EnglishLemmaFilter/Filter</key>
        <resourceName>EnglishFilter</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>EnglishCompoundSplitter/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermCleaner/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
