<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="EnglishTreeTaggerCaseNormalizer">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerCaseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerGenderNormalizer">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerGenderNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerTenseNormalizer">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerTenseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerSubcategoryNormalizer">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerSubcategoryNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerNumberNormalizer">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerNumberNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerMoodNormalizer">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerMoodNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerCategoryNormalizer">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerCategoryNormalizer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>English TreeTagger Normalizer</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>CategoryMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerCategoryNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SubcategoryMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerSubcategoryNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>MoodMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerMoodNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TenseMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerTenseNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>GenderMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerGenderNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>NumberMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerNumberNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>CaseMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerCaseNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>EnglishTreeTaggerCategoryNormalizer</node>
        <node>EnglishTreeTaggerSubcategoryNormalizer</node>
        <node>EnglishTreeTaggerMoodNormalizer</node>
        <node>EnglishTreeTaggerTenseNormalizer</node>
        <node>EnglishTreeTaggerGenderNormalizer</node>
        <node>EnglishTreeTaggerNumberNormalizer</node>
        <node>EnglishTreeTaggerCaseNormalizer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>en</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
