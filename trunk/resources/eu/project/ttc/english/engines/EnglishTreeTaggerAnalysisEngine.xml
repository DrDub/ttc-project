<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="EnglishStemmer">
      <import name="eu.project.ttc.english.engines.EnglishStemmer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerTenseNormalizer">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerTenseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerSubcategoryNormalizer">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerSubcategoryNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishWordTokenizer">
      <import name="eu.project.ttc.english.engines.EnglishWordTokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerMoodNormalizer">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerMoodNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="XmiCasConsumer">
      <import name="eu.project.ttc.all.engines.XmiCasConsumer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerWrapper">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerWrapper"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerCategoryNormalizer">
      <import name="eu.project.ttc.english.engines.EnglishTreeTaggerCategoryNormalizer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>English TreeTagger Analysis Engine</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>TreeTaggerHomeDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerWrapper/TreeTaggerHomeDirectory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TreeTaggerParameterFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerWrapper/TreeTaggerParameterFile</parameter>
        </overrides>
      </configurationParameter>      
      <configurationParameter>
        <name>SegmentFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishWordTokenizer/File</parameter>
        </overrides>
      </configurationParameter>      
      <configurationParameter>
        <name>CategoryMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerCategoryNormalizer/File</parameter>
        </overrides>
      </configurationParameter>      
      <configurationParameter>
        <name>SubcategoryMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerSubcategoryNormalizer/File</parameter>
        </overrides>
      </configurationParameter>      
      <configurationParameter>
        <name>MoodMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerMoodNormalizer/File</parameter>
        </overrides>
      </configurationParameter>      
      <configurationParameter>
        <name>TenseMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerTenseNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Directory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>XmiCasConsumer/Directory</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>TreeTaggerHomeDirectory</name>
        <value>
          <string>/usr/share/tree-tagger</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Directory</name>
        <value>
          <string>/tmp</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>EnglishWordTokenizer</node>
        <node>EnglishTreeTaggerWrapper</node>
        <node>EnglishTreeTaggerCategoryNormalizer</node>
        <node>EnglishTreeTaggerSubcategoryNormalizer</node>
        <node>EnglishTreeTaggerMoodNormalizer</node>
        <node>EnglishTreeTaggerTenseNormalizer</node>
        <node>EnglishStemmer</node>
        <node>XmiCasConsumer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>en</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
