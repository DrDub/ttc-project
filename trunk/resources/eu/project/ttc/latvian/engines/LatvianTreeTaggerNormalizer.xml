<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="LatvianTreeTaggerCaseNormalizer">
      <import name="eu.project.ttc.latvian.engines.LatvianTreeTaggerCaseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="LatvianTreeTaggerGenderNormalizer">
      <import name="eu.project.ttc.latvian.engines.LatvianTreeTaggerGenderNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="LatvianTreeTaggerTenseNormalizer">
      <import name="eu.project.ttc.latvian.engines.LatvianTreeTaggerTenseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="LatvianTreeTaggerSubcategoryNormalizer">
      <import name="eu.project.ttc.latvian.engines.LatvianTreeTaggerSubcategoryNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="LatvianTreeTaggerNumberNormalizer">
      <import name="eu.project.ttc.latvian.engines.LatvianTreeTaggerNumberNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="LatvianTreeTaggerMoodNormalizer">
      <import name="eu.project.ttc.latvian.engines.LatvianTreeTaggerMoodNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="LatvianTreeTaggerCategoryNormalizer">
      <import name="eu.project.ttc.latvian.engines.LatvianTreeTaggerCategoryNormalizer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>Latvian TreeTagger Normalizer</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>CategoryMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>LatvianTreeTaggerCategoryNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SubcategoryMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>LatvianTreeTaggerSubcategoryNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>MoodMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>LatvianTreeTaggerMoodNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TenseMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>LatvianTreeTaggerTenseNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>GenderMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>LatvianTreeTaggerGenderNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>NumberMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>LatvianTreeTaggerNumberNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>CaseMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>LatvianTreeTaggerCaseNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>LatvianTreeTaggerCategoryNormalizer</node>
        <node>LatvianTreeTaggerSubcategoryNormalizer</node>
        <node>LatvianTreeTaggerMoodNormalizer</node>
        <node>LatvianTreeTaggerTenseNormalizer</node>
        <node>LatvianTreeTaggerGenderNormalizer</node>
        <node>LatvianTreeTaggerNumberNormalizer</node>
        <node>LatvianTreeTaggerCaseNormalizer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>lv</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
