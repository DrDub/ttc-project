<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>Term Suite Type System</name>
  <description>This Type System gathers Type Systems required by every UIMA CAS processors used within the TTC project.</description>
  <version>1.0</version>
  <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
  <imports>
    <import name="fr.univnantes.lina.uima.types.TermAnnotation"/>
    <import name="org.apache.uima.examples.SourceDocumentInformation"/>
  </imports>
  <types>
    <typeDescription>
      <name>eu.project.ttc.types.WordAnnotation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>category</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lemma</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
