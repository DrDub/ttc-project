<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="WordTokenizer">
      <import name="fr.univnantes.lina.uima.engines.WordTokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TreeTagger">
      <import name="fr.univnantes.lina.uima.engines.TreeTagger"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TreeTaggerLemmaAnnotator">
      <import name="eu.project.ttc.engines.TreeTaggerLemmaAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TreeTaggerMultextAnnotator">
      <import name="fr.univnantes.lina.uima.engines.TreeTaggerMultextAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="Stemmer">
      <import name="fr.univnantes.lina.uima.engines.Stemmer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>Preliminary Linguistic Analysis</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>SofaNames</name>
        <description>The Sofa names the annotator should work on. If no
					names are specified, the annotator works on the
					default sofa.</description>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>WordTokenizer/SofaNames</parameter>
          <parameter>TreeTagger/SofaNames</parameter>
          <parameter>TreeTaggerMultextAnnotator/SofaNames</parameter>
          <parameter>TreeTaggerLemmaAnnotator/SofaNames</parameter>
          <parameter>Stemmer/SofaNames</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TreeTaggerHomeDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>TreeTagger/TreeTaggerHomeDirectory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>ExpandPartOfSpeech</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>TreeTaggerMultextAnnotator/ExpandPartOfSpeech</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>SofaNames</name>
        <value>
          <array>
            <string>_InitialView</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>ExpandPartOfSpeech</name>
        <value>
          <boolean>true</boolean>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>WordTokenizer</node>
        <node>TreeTagger</node>
        <node>TreeTaggerMultextAnnotator</node>
        <node>TreeTaggerLemmaAnnotator</node>
        <node>Stemmer</node>
      </fixedFlow>
    </flowConstraints>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">org.apache.uima.TokenAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.SentenceAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.TreeTaggerAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.MultextAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.LemmaAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>en</language>
          <language>fr</language>
          <language>de</language>
          <language>es</language>
          <language>ru</language>
          <language>lv</language>
          <language>zh</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
