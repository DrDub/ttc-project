<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="EditDistanceGathering">
      <import name="eu.project.ttc.all.engines.EditDistanceGathering"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchRuleBasedGathering">
      <import name="eu.project.ttc.french.engines.FrenchRuleBasedGathering"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>French Term Gatherer</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>Type</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>EditDistanceGathering/Type</parameter>
          <parameter>FrenchRuleBasedGathering/Type</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>EditDistanceClassName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>EditDistanceGathering/EditDistanceClassName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>File</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>FrenchRuleBasedGathering/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Override</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>FrenchRuleBasedGathering/Override</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Enable</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>FrenchRuleBasedGathering/Enable</parameter>
          <parameter>EditDistanceGathering/Enable</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>Type</name>
        <value>
          <string>eu.project.ttc.types.TermAnnotation</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>EditDistanceClassName</name>
        <value>
          <string>eu.project.ttc.metrics.LevenshteinDistance</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Override</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Enable</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>EditDistanceGathering</node>
        <node>FrenchRuleBasedGathering</node>
      </fixedFlow>
    </flowConstraints>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>DefaultIndex</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/all/engines/IndexWriter.xml</fileUrl>
        </fileResourceSpecifier>
        <implementationName>uima.sandbox.indexer.resources.DefaultIndex</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>EditDistanceGathering/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>FrenchRuleBasedGathering/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
