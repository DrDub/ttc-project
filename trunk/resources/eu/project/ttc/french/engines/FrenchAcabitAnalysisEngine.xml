<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="FrenchTermIndexer">
      <import name="eu.project.ttc.french.engines.FrenchTermIndexer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTermDetector">
      <import name="eu.project.ttc.french.engines.FrenchTermDetector"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTermFilter">
      <import name="eu.project.ttc.french.engines.FrenchTermFilter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchCompoundSplitter">
      <import name="eu.project.ttc.french.engines.FrenchCompoundSplitter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="IndexWriter">
      <import name="eu.project.ttc.all.engines.IndexWriter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="XmiCasConsumer">
      <import name="eu.project.ttc.all.engines.XmiCasConsumer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTermGatherer">
      <import name="eu.project.ttc.french.engines.FrenchTermGatherer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchLemmaFilter">
      <import name="eu.project.ttc.french.engines.FrenchLemmaFilter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermIndexer">
      <import name="eu.project.ttc.all.engines.TermIndexer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>French Acabit Analysis Engine</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>Directory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>XmiCasConsumer/Directory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>File</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>IndexWriter/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Type</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermIndexer/Type</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Language</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermIndexer/Language</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TermVariationRuleFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>FrenchTermGatherer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>MultiWordPatternRuleFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>FrenchTermDetector/MultiWordPatternRuleFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>NeoclassicalElementFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>FrenchTermDetector/NeoclassicalElementFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Dictionary</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>FrenchCompoundSplitter/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>EditDistanceClassName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>FrenchTermGatherer/EditDistanceClassName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>EnableTermGathering</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>FrenchTermGatherer/Enable</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>EditDistanceThreshold</name>
        <type>Float</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>FrenchTermGatherer/Threshold</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>EditDistanceNgrams</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>FrenchTermGatherer/Ngrams</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>Directory</name>
        <value>
          <string>/tmp</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>File</name>
        <value>
          <string>fr-terminology.xmi</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Type</name>
        <value>
          <string>eu.project.ttc.types.TermAnnotation</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Language</name>
        <value>
          <string>fr</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>EditDistanceClassName</name>
        <value>
          <string>eu.project.ttc.metrics.LevenshteinDistance</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>EnableTermGathering</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>EditDistanceThreshold</name>
        <value>
          <float>0.5</float>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>EditDistanceNgrams</name>
        <value>
          <integer>1</integer>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>FrenchTermDetector</node>
        <node>FrenchTermFilter</node>
        <node>FrenchLemmaFilter</node>
        <node>TermIndexer</node>
        <node>FrenchTermIndexer</node>
        <node>FrenchCompoundSplitter</node>
        <node>FrenchTermGatherer</node>
        <node>IndexWriter</node>
        <node>XmiCasConsumer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.SourceDocumentInformation</type>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </inputs>
        <outputs/>
        <languagesSupported>
          <language>fr</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>DefaultIndex</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/all/engines/IndexWriter.xml</fileUrl>
        </fileResourceSpecifier>
        <implementationName>uima.sandbox.indexer.resources.DefaultIndex</implementationName>
      </externalResource>
      <externalResource>
        <name>FrenchFilter</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/french/resources/french-stop-word-filter.xml</fileUrl>
        </fileResourceSpecifier>
        <implementationName>uima.sandbox.filter.resources.FilterResource</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>FrenchTermGatherer/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>FrenchTermIndexer/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermIndexer/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>IndexWriter/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>FrenchTermFilter/Filter</key>
        <resourceName>FrenchFilter</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>FrenchLemmaFilter/Filter</key>
        <resourceName>FrenchFilter</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>FrenchCompoundSplitter/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
