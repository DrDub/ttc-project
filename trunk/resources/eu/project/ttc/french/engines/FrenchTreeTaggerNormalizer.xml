<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="FrenchTreeTaggerCaseNormalizer">
      <import name="eu.project.ttc.french.engines.FrenchTreeTaggerCaseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTreeTaggerGenderNormalizer">
      <import name="eu.project.ttc.french.engines.FrenchTreeTaggerGenderNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTreeTaggerTenseNormalizer">
      <import name="eu.project.ttc.french.engines.FrenchTreeTaggerTenseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTreeTaggerSubcategoryNormalizer">
      <import name="eu.project.ttc.french.engines.FrenchTreeTaggerSubcategoryNormalizer"/>
