          <parameter>FrenchTreeTaggerGenderNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>NumberMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>FrenchTreeTaggerNumberNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>CaseMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>FrenchTreeTaggerCaseNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>FrenchTreeTaggerCategoryNormalizer</node>
        <node>FrenchTreeTaggerSubcategoryNormalizer</node>
        <node>FrenchTreeTaggerMoodNormalizer</node>
        <node>FrenchTreeTaggerTenseNormalizer</node>
        <node>FrenchTreeTaggerGenderNormalizer</node>
        <node>FrenchTreeTaggerNumberNormalizer</node>
        <node>FrenchTreeTaggerCaseNormalizer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>fr</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
