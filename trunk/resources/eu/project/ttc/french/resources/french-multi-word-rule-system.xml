<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<patterns>
	<pattern id="noun-noun">
		<sequence>
			<patterns>
				<pattern>
					<annotation type="eu.project.ttc.types.WordAnnotation">
					<constraint>
						<eq key="category" value="noun" />
					</constraint>
					</annotation>
				</pattern>
				<pattern>
					<annotation type="eu.project.ttc.types.WordAnnotation">
						<constraint>
							<or>
								<constraints>
									<constraint>
										<eq key="category" value="noun" />
									</constraint>
									<constraint>
										<eq key="category" value="name" />
									</constraint>
								</constraints>
							</or>
						</constraint>
					</annotation>
				</pattern>
			</patterns>
		</sequence>
	</pattern>
