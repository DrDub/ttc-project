<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>eu.project.ttc.engines.TreeTaggerLemmaAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>TreeTagger Lemma Annotator</name>
    <description>This Analysis Engine maps the feature 'lemma' of annotations which type is 'org.apache.uima.TreeTaggerAnnotation' into the feature 'value' of annotations which type is 'org.apache.uima.LemmaAnnotation'.</description>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters/>
    <configurationParameterSettings/>
    <typeSystemDescription>
      <imports>
        <import name="fr.univnantes.lina.uima.types.LemmaAnnotation"/>
        <import name="fr.univnantes.lina.uima.types.TreeTaggerAnnotation"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type>org.apache.uima.TreeTaggerAnnotation</type>
          <feature>org.apache.uima.TreeTaggerAnnotation:lemma</feature>
        </inputs>
        <outputs>
          <type>org.apache.uima.LemmaAnnotation</type>
          <feature>org.apache.uima.LemmaAnnotation:found</feature>
          <feature>org.apache.uima.LemmaAnnotation:value</feature>
        </outputs>
        <languagesSupported>
          <language>en</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
