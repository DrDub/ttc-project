<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>fr.univnantes.lina.uima.engines.TermNoteAnnotator</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>French Term Lemma Annotator</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>AnnotationType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>NoteName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>AnnotationType</name>
        <value>
          <string>org.apache.uima.LemmaAnnotation:value</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>NoteName</name>
        <value>
          <string>lemma</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import name="fr.univnantes.lina.uima.types.LemmaAnnotation"/>
        <import name="fr.univnantes.lina.uima.types.TermAnnotation"/>
      </imports>
    </typeSystemDescription>
    <typePriorities>
      <priorityList>
        <type>org.apache.uima.TermAnnotation</type>
        <type>org.apache.uima.TermComponentAnnotation</type>
        <type>org.apache.uima.LemmaAnnotation</type>
      </priorityList>
    </typePriorities>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.apache.uima.LemmaAnnotation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">org.apache.uima.TermNoteAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>fr</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
