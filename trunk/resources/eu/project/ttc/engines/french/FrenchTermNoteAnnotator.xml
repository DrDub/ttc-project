<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="FrenchTermLemmaAnnotator">
      <import name="eu.project.ttc.engines.french.FrenchTermLemmaAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTermPartOfSpeechAnnotator">
      <import name="eu.project.ttc.engines.french.FrenchTermPartOfSpeechAnnotator"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <flowController key="AnnotationTypeFlowController">
    <import name="eu.project.ttc.controllers.AnnotationTypeFlowController"/>
  </flowController>
  <analysisEngineMetaData>
    <name>French Term Note Annotator</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters/>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>FrenchTermPartOfSpeechAnnotator</node>
        <node>FrenchTermLemmaAnnotator</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.apache.uima.MultextAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.LemmaAnnotation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">org.apache.uima.TermNoteAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>fr</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
