<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="FrenchTreeTaggerMultextMapper">
      <import name="eu.project.ttc.engines.french.FrenchTreeTaggerMultextMapper"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TreeTaggerLemmaAnnotator">
      <import name="eu.project.ttc.engines.TreeTaggerLemmaAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchWordTokenizer">
      <import name="eu.project.ttc.engines.french.FrenchWordTokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchRuleBasedTermDetector">
      <import name="eu.project.ttc.engines.french.FrenchRuleBasedTermDetector"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTreeTaggerWrapper">
      <import name="eu.project.ttc.engines.french.FrenchTreeTaggerWrapper"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>French Linguistic Analysis</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>TreeTaggerHomeDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>FrenchTreeTaggerWrapper/TreeTaggerHomeDirectory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>AnnotationType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>FrenchRuleBasedTermDetector/AnnotationType</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>AnnotationType</name>
        <value>
          <string>org.apache.uima.MultextAnnotation</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>FrenchWordTokenizer</node>
        <node>FrenchTreeTaggerWrapper</node>
        <node>FrenchTreeTaggerMultextMapper</node>
        <node>TreeTaggerLemmaAnnotator</node>
        <node>FrenchRuleBasedTermDetector</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">org.apache.uima.WordAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.TreeTaggerAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.MultextAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.TermAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>fr</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
