<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="FrenchTermDetector">
      <import name="eu.project.ttc.engines.french.FrenchTermDetector"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTermFilter">
      <import name="eu.project.ttc.engines.french.FrenchTermFilter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTreeTaggerCategoryNormalizer">
      <import name="eu.project.ttc.engines.french.FrenchTreeTaggerCategoryNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchWordTokenizer">
      <import name="eu.project.ttc.engines.french.FrenchWordTokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTreeTaggerWrapper">
      <import name="eu.project.ttc.engines.french.FrenchTreeTaggerWrapper"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>French Linguistic Analysis</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>TreeTaggerHomeDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>FrenchTreeTaggerWrapper/TreeTaggerHomeDirectory</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>FrenchWordTokenizer</node>
        <node>FrenchTreeTaggerWrapper</node>
        <node>FrenchTreeTaggerCategoryNormalizer</node>
        <node>FrenchTermDetector</node>
        <node>FrenchTermFilter</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.SourceDocumentInformation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.TermAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>fr</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
