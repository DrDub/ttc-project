<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="GermanTermDetector">
      <import name="eu.project.ttc.engines.german.GermanTermDetector"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanWordTokenizer">
      <import name="eu.project.ttc.engines.german.GermanWordTokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanTermFilter">
      <import name="eu.project.ttc.engines.german.GermanTermFilter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanTreeTaggerMultextMapper">
      <import name="eu.project.ttc.engines.german.GermanTreeTaggerMultextMapper"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanTreeTaggerWrapper">
      <import name="eu.project.ttc.engines.german.GermanTreeTaggerWrapper"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>German Linguistic Analysis</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>TreeTaggerHomeDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>GermanTreeTaggerWrapper/TreeTaggerHomeDirectory</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>GermanWordTokenizer</node>
        <node>GermanTreeTaggerWrapper</node>
        <node>GermanTreeTaggerMultextMapper</node>
        <node>GermanTermDetector</node>
        <node>GermanTermFilter</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.SourceDocumentInformation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.TermAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>de</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
