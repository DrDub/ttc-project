<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>eu.project.ttc.engines.aligner.TermContextIndexer</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>Term Context Indexer</name>
    <description/>
    <version/>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>SourceLanguage</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>SourceName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TargetLanguage</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TargetName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>SourceLanguage</name>
        <value>
          <string>en</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TargetLanguage</name>
        <value>
          <string>fr</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TargetName</name>
        <value>
          <string>Target</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>SourceName</name>
        <value>
          <string>Source</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import name="eu.project.ttc.types.TermSuiteTypeSystem"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.SingleWordTermAnnotation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.TermContextEntryAnnotation</type>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.TermContextIndexAnnotation</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>true</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <externalResourceDependencies>
    <externalResourceDependency>
      <key>TermContextBench</key>
      <description/>
      <interfaceName>eu.project.ttc.models.aligner.TermContextBench</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
  </externalResourceDependencies>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>DefaultTermContextBench</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/models/aligner/TermContextBench.class</fileUrl>
        </fileResourceSpecifier>
        <implementationName>eu.project.ttc.models.aligner.TermContextBenchResource</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>TermContextBench</key>
        <resourceName>DefaultTermContextBench</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
