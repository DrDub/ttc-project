<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="TreeTaggerLemmaAnnotator">
      <import name="eu.project.ttc.engines.TreeTaggerLemmaAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianTreeTaggerMultextMapper">
      <import name="eu.project.ttc.engines.russian.RussianTreeTaggerMultextMapper"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianTermDetector">
      <import name="eu.project.ttc.engines.russian.RussianTermDetector"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianTreeTaggerWrapper">
      <import name="eu.project.ttc.engines.russian.RussianTreeTaggerWrapper"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianWordTokenizer">
      <import name="eu.project.ttc.engines.russian.RussianWordTokenizer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>Russian Linguistic Analysis</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>TreeTaggerHomeDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>RussianTreeTaggerWrapper/TreeTaggerHomeDirectory</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>RussianWordTokenizer</node>
        <node>RussianTreeTaggerWrapper</node>
        <node>RussianTreeTaggerMultextMapper</node>
        <node>TreeTaggerLemmaAnnotator</node>
        <node>RussianTermDetector</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.SourceDocumentInformation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">org.apache.uima.WordAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.TreeTaggerAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.MultextAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.LemmaAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.TermAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>ru</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
