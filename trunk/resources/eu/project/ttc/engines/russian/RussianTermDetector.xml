<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="RussianNeoClassicalTermDetector">
      <import name="eu.project.ttc.engines.russian.RussianNeoClassicalTermDetector"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianMultiWordTermDetector">
      <import name="eu.project.ttc.engines.russian.RussianMultiWordTermDetector"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SingleWordTermDetector">
      <import name="eu.project.ttc.engines.SingleWordTermDetector"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>Russian Term Detector</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback"/>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>SingleWordTermDetector</node>
        <node>RussianMultiWordTermDetector</node>
        <node>RussianNeoClassicalTermDetector</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.SourceDocumentInformation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">org.apache.uima.TermComponentAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.TermAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>ru</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
