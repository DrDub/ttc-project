<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="EnglishWordTokenizer">
      <import name="eu.project.ttc.engines.english.EnglishWordTokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerWrapper">
      <import name="eu.project.ttc.engines.english.EnglishTreeTaggerWrapper"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTermFilter">
      <import name="eu.project.ttc.engines.english.EnglishTermFilter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTermDetector">
      <import name="eu.project.ttc.engines.english.EnglishTermDetector"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerCategoryNormalizer">
      <import name="eu.project.ttc.engines.english.EnglishTreeTaggerCategoryNormalizer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>English Linguistic Analysis</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>TreeTaggerHomeDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>EnglishTreeTaggerWrapper/TreeTaggerHomeDirectory</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>EnglishWordTokenizer</node>
        <node>EnglishTreeTaggerWrapper</node>
        <node>EnglishTreeTaggerCategoryNormalizer</node>
        <node>EnglishTermDetector</node>
        <node>EnglishTermFilter</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.SourceDocumentInformation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.TermAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>en</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
