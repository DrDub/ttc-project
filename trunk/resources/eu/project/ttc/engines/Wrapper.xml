<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>eu.project.ttc.engines.Wrapper</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>Wrapper</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>WrapperConfigurationFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings/>
    <typeSystemDescription>
      <imports>
        <import name="fr.univnantes.lina.uima.types.MultextAnnotation"/>
        <import name="fr.univnantes.lina.uima.types.LemmaAnnotation"/>
        <import name="fr.univnantes.lina.uima.types.TermAnnotation"/>
        <import name="org.apache.uima.examples.SourceDocumentInformation"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.SourceDocumentInformation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.MultextAnnotation</type>
          <type allAnnotatorFeatures="true">org.apache.uima.LemmaAnnotation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">org.apache.uima.TermAnnotation</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <externalResourceDependencies>
    <externalResourceDependency>
      <key>WrapperConfiguration</key>
      <description/>
      <interfaceName>eu.project.ttc.models.WrapperConfiguration</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
  </externalResourceDependencies>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>DefaultWrapperConfiguration</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/resources/WrapperConfiguration.Default</fileUrl>
        </fileResourceSpecifier>
        <implementationName>eu.project.ttc.models.WrapperConfiguration</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>WrapperConfiguration</key>
        <resourceName>DefaultWrapperConfiguration</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
