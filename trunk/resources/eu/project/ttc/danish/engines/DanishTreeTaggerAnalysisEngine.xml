<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="DanishStemmer">
      <import name="eu.project.ttc.danish.engines.DanishStemmer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishTreeTaggerNormalizer">
      <import name="eu.project.ttc.danish.engines.DanishTreeTaggerNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishWordTokenizer">
      <import name="eu.project.ttc.danish.engines.DanishWordTokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="XmiCasConsumer">
      <import name="eu.project.ttc.all.engines.XmiCasConsumer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishTreeTaggerWrapper">
      <import name="eu.project.ttc.danish.engines.DanishTreeTaggerWrapper"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>Danish TreeTagger Analysis Engine</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>TreeTaggerHomeDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerWrapper/TreeTaggerHomeDirectory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TreeTaggerParameterFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerWrapper/TreeTaggerParameterFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SegmentFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishWordTokenizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Directory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>XmiCasConsumer/Directory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>CategoryMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerNormalizer/CategoryMappingFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SubcategoryMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerNormalizer/SubcategoryMappingFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TenseMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerNormalizer/TenseMappingFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>MoodMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerNormalizer/MoodMappingFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>GenderMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerNormalizer/GenderMappingFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>NumberMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerNormalizer/NumberMappingFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>CaseMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerNormalizer/CaseMappingFile</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>TreeTaggerHomeDirectory</name>
        <value>
          <string>/usr/share/tree-tagger</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Directory</name>
        <value>
          <string>/tmp</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>DanishWordTokenizer</node>
        <node>DanishTreeTaggerWrapper</node>
        <node>DanishTreeTaggerNormalizer</node>
        <node>DanishStemmer</node>
        <node>XmiCasConsumer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>da</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
