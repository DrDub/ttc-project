<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="DanishTermIndexer">
      <import name="eu.project.ttc.danish.engines.DanishTermIndexer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishTermDetector">
      <import name="eu.project.ttc.danish.engines.DanishTermDetector"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishTermFilter">
      <import name="eu.project.ttc.danish.engines.DanishTermFilter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishCompoundSplitter">
      <import name="eu.project.ttc.danish.engines.DanishCompoundSplitter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="IndexWriter">
      <import name="eu.project.ttc.all.engines.IndexWriter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="XmiCasConsumer">
      <import name="eu.project.ttc.all.engines.XmiCasConsumer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishTermGatherer">
      <import name="eu.project.ttc.danish.engines.DanishTermGatherer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishLemmaFilter">
      <import name="eu.project.ttc.danish.engines.DanishLemmaFilter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermIndexer">
      <import name="eu.project.ttc.all.engines.TermIndexer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>Danish Acabit Analysis Engine</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>Directory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>XmiCasConsumer/Directory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>File</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>IndexWriter/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Type</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermIndexer/Type</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Language</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermIndexer/Language</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TermVariationRuleFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTermGatherer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>MultiWordPatternRuleFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTermDetector/MultiWordPatternRuleFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>NeoclassicalElementFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTermDetector/NeoclassicalElementFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Dictionary</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishCompoundSplitter/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>EditDistanceClassName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>DanishTermGatherer/EditDistanceClassName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>EnableTermGathering</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>DanishTermGatherer/Enable</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>Directory</name>
        <value>
          <string>/tmp</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>File</name>
        <value>
          <string>da-terminology.xmi</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Type</name>
        <value>
          <string>eu.project.ttc.types.TermAnnotation</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Language</name>
        <value>
          <string>da</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>EditDistanceClassName</name>
        <value>
          <string>eu.project.ttc.metrics.LevenshteinDistance</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>EnableTermGathering</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>DanishTermDetector</node>
        <node>DanishTermFilter</node>
        <node>DanishLemmaFilter</node>
        <node>TermIndexer</node>
        <node>DanishTermIndexer</node>
        <node>DanishCompoundSplitter</node>
        <node>DanishTermGatherer</node>
        <node>IndexWriter</node>
        <node>XmiCasConsumer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.apache.uima.examples.SourceDocumentInformation</type>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </inputs>
        <outputs/>
        <languagesSupported>
          <language>da</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>DefaultIndex</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/all/engines/IndexWriter.xml</fileUrl>
        </fileResourceSpecifier>
        <implementationName>uima.sandbox.indexer.resources.DefaultIndex</implementationName>
      </externalResource>
      <externalResource>
        <name>DanishFilter</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/danish/resources/danish-stop-word-filter.xml</fileUrl>
        </fileResourceSpecifier>
        <implementationName>uima.sandbox.filter.resources.FilterResource</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>DanishTermGatherer/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>DanishTermIndexer/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermIndexer/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>IndexWriter/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>DanishTermFilter/Filter</key>
        <resourceName>DanishFilter</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>DanishLemmaFilter/Filter</key>
        <resourceName>DanishFilter</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>DanishCompoundSplitter/Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
