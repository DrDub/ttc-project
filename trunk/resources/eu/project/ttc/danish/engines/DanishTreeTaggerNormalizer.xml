    <delegateAnalysisEngine key="DanishTreeTaggerCategoryNormalizer">
      <import name="eu.project.ttc.danish.engines.DanishTreeTaggerCategoryNormalizer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>Danish TreeTagger Normalizer</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>CategoryMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerCategoryNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SubcategoryMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerSubcategoryNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>MoodMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerMoodNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TenseMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerTenseNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>GenderMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerGenderNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>NumberMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerNumberNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>CaseMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>DanishTreeTaggerCaseNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>DanishTreeTaggerCategoryNormalizer</node>
        <node>DanishTreeTaggerSubcategoryNormalizer</node>
        <node>DanishTreeTaggerMoodNormalizer</node>
        <node>DanishTreeTaggerTenseNormalizer</node>
        <node>DanishTreeTaggerGenderNormalizer</node>
        <node>DanishTreeTaggerNumberNormalizer</node>
        <node>DanishTreeTaggerCaseNormalizer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>da</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
