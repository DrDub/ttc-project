<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="DanishTreeTaggerCaseNormalizer">
      <import name="eu.project.ttc.danish.engines.DanishTreeTaggerCaseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishTreeTaggerGenderNormalizer">
      <import name="eu.project.ttc.danish.engines.DanishTreeTaggerGenderNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishTreeTaggerTenseNormalizer">
      <import name="eu.project.ttc.danish.engines.DanishTreeTaggerTenseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishTreeTaggerSubcategoryNormalizer">
      <import name="eu.project.ttc.danish.engines.DanishTreeTaggerSubcategoryNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishTreeTaggerNumberNormalizer">
      <import name="eu.project.ttc.danish.engines.DanishTreeTaggerNumberNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="DanishTreeTaggerMoodNormalizer">
      <import name="eu.project.ttc.danish.engines.DanishTreeTaggerMoodNormalizer"/>
