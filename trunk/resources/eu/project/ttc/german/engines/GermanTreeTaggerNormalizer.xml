      </configurationParameter>
      <configurationParameter>
        <name>SubcategoryMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>GermanTreeTaggerSubcategoryNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>MoodMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>GermanTreeTaggerMoodNormalizer/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TenseMappingFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
