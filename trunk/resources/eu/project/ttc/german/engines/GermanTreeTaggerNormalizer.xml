  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="GermanTreeTaggerCaseNormalizer">
      <import name="eu.project.ttc.german.engines.GermanTreeTaggerCaseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanTreeTaggerGenderNormalizer">
      <import name="eu.project.ttc.german.engines.GermanTreeTaggerGenderNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanTreeTaggerTenseNormalizer">
      <import name="eu.project.ttc.german.engines.GermanTreeTaggerTenseNormalizer"/>
