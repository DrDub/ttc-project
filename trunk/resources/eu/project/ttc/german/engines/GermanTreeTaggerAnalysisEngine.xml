<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="GermanStemmer">
      <import name="eu.project.ttc.german.engines.GermanStemmer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanTreeTaggerTenseNormalizer">
      <import name="eu.project.ttc.german.engines.GermanTreeTaggerTenseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanTreeTaggerSubcategoryNormalizer">
      <import name="eu.project.ttc.german.engines.GermanTreeTaggerSubcategoryNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanWordTokenizer">
      <import name="eu.project.ttc.german.engines.GermanWordTokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanTreeTaggerMoodNormalizer">
      <import name="eu.project.ttc.german.engines.GermanTreeTaggerMoodNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="XmiCasConsumer">
      <import name="eu.project.ttc.all.engines.XmiCasConsumer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanTreeTaggerWrapper">
      <import name="eu.project.ttc.german.engines.GermanTreeTaggerWrapper"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanTreeTaggerCategoryNormalizer">
      <import name="eu.project.ttc.german.engines.GermanTreeTaggerCategoryNormalizer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>German TreeTagger Analysis Engine</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>TreeTaggerHomeDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>GermanTreeTaggerWrapper/TreeTaggerHomeDirectory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Directory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>XmiCasConsumer/Directory</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>TreeTaggerHomeDirectory</name>
        <value>
          <string>/usr/share/tree-tagger</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Directory</name>
        <value>
          <string>/tmp</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>GermanWordTokenizer</node>
        <node>GermanTreeTaggerWrapper</node>
        <node>GermanTreeTaggerCategoryNormalizer</node>
        <node>GermanTreeTaggerSubcategoryNormalizer</node>
        <node>GermanTreeTaggerMoodNormalizer</node>
        <node>GermanTreeTaggerTenseNormalizer</node>
        <node>GermanStemmer</node>
        <node>XmiCasConsumer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>de</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
