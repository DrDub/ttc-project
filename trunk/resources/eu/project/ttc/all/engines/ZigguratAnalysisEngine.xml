<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>eu.project.ttc.engines.ZigguratAnalysisEngine</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>Ziggurat Analysis Engine</name>
    <description/>
    <version/>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>DictionaryFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>SourceTermContextIndexFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TargetTermContextIndexFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>SimilarityDistanceClassName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>SimilarityDistanceClassName</name>
        <value>
          <string>eu.project.ttc.metrics.Cosinus</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import name="eu.project.ttc.types.TermSuiteTypeSystem"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>false</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <externalResourceDependencies>
    <externalResourceDependency>
      <key>Dictionary</key>
      <description/>
      <interfaceName>fr.univnantes.lina.uima.dictionaries.Dictionary</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
    <externalResourceDependency>
      <key>SourceIndex</key>
      <description/>
      <interfaceName>eu.project.ttc.models.TermContextIndex</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
    <externalResourceDependency>
      <key>TargetIndex</key>
      <description/>
      <interfaceName>eu.project.ttc.models.TermContextIndex</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
  </externalResourceDependencies>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>DefaultDictionary</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>fr/univnantes/lina/uima/resources/Dictionary.Default</fileUrl>
        </fileResourceSpecifier>
        <implementationName>fr.univnantes.lina.uima.dictionaries.DictionaryResource</implementationName>
      </externalResource>
      <externalResource>
        <name>SourceTermContextIndex</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/models/TermContextIndexListener.class</fileUrl>
        </fileResourceSpecifier>
        <implementationName>eu.project.ttc.models.TermContextIndexListener</implementationName>
      </externalResource>
      <externalResource>
        <name>TargetTermContextIndex</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/models/TermContextIndexListener.class</fileUrl>
        </fileResourceSpecifier>
        <implementationName>eu.project.ttc.models.TermContextIndexListener</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>Dictionary</key>
        <resourceName>DefaultDictionary</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>SourceIndex</key>
        <resourceName>SourceTermContextIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TargetIndex</key>
        <resourceName>TargetTermContextIndex</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
