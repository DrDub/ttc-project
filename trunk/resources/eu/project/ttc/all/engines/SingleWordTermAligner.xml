          <fileUrl>fr/univnantes/lina/uima/resources/Dictionary.Default</fileUrl>
        </fileResourceSpecifier>
        <implementationName>fr.univnantes.lina.uima.dictionaries.DictionaryResource</implementationName>
      </externalResource>
      <externalResource>
        <name>EvaluationDictionary</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/all/resources/Dictionary.Evaluation</fileUrl>
        </fileResourceSpecifier>
        <implementationName>fr.univnantes.lina.uima.dictionaries.DictionaryResource</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>TermContextComputer/TermContextSpace</key>
        <resourceName>DefaultTermContextSpace</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermContextAligner/TermContextSpace</key>
        <resourceName>DefaultTermContextSpace</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermContextFilter/TermContextSpace</key>
        <resourceName>DefaultTermContextSpace</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermContextFilter/Dictionary</key>
        <resourceName>EvaluationDictionary</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermContextChecker/TermContextSpace</key>
        <resourceName>DefaultTermContextSpace</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermContextChecker/Dictionary</key>
        <resourceName>EvaluationDictionary</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermContextShrinker/TermContextSpace</key>
        <resourceName>DefaultTermContextSpace</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermContextShrinker/Dictionary</key>
        <resourceName>DefaultDictionary</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermContextTransferer/TermContextSpace</key>
        <resourceName>DefaultTermContextSpace</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermContextTransferer/Dictionary</key>
        <resourceName>DefaultDictionary</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>SourceTermContextNormalizer/TermContextSpace</key>
        <resourceName>DefaultTermContextSpace</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TargetTermContextNormalizer/TermContextSpace</key>
        <resourceName>DefaultTermContextSpace</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
