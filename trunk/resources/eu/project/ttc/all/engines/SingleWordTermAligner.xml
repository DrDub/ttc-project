<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="TargetTermContextNormalizer">
      <import name="eu.project.ttc.all.engines.TermContextNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermContextChecker">
      <import name="eu.project.ttc.all.engines.TermContextChecker"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermContextComputer">
      <import name="eu.project.ttc.all.engines.TermContextComputer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermContextShrinker">
      <import name="eu.project.ttc.all.engines.TermContextShrinker"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermContextTransferer">
      <import name="eu.project.ttc.all.engines.TermContextTransferer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermContextFilter">
      <import name="eu.project.ttc.all.engines.TermContextFilter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SourceTermContextNormalizer">
      <import name="eu.project.ttc.all.engines.TermContextNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermContextAligner">
      <import name="eu.project.ttc.all.engines.TermContextAligner"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermContextBuilder">
      <import name="eu.project.ttc.all.engines.TermContextBuilder"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>SingleWord Term Aligner</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>AssociationRateClassName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>SourceTermContextNormalizer/AssociationRateClassName</parameter>
          <parameter>TargetTermContextNormalizer/AssociationRateClassName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SimilarityDistanceClassName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermContextAligner/SimilarityDistanceClassName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>DictionaryFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>TermContextTransferer/DictionaryFile</parameter>
          <parameter>TermContextShrinker/DictionaryFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>File</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermContextChecker/File</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>EvaluationListFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>TermContextFilter/DictionaryFile</parameter>
          <parameter>TermContextChecker/DictionaryFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>SourceName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>SourceTermContextNormalizer/IndexName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TargetName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TargetTermContextNormalizer/IndexName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>NormalizedSourceName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>SourceTermContextNormalizer/ResultName</parameter>
          <parameter>TermContextFilter/IndexName</parameter>
          <parameter>TermContextShrinker/SourceName</parameter>
          <parameter>TermContextTransferer/SourceName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>NormalizedTargetName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TargetTermContextNormalizer/ResultName</parameter>
          <parameter>TermContextShrinker/TargetName</parameter>
          <parameter>TermContextTransferer/TargetName</parameter>
          <parameter>TermContextAligner/TargetName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TransferedSourceName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermContextTransferer/ResultName</parameter>
          <parameter>TermContextAligner/SourceName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>ResultName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermContextAligner/ResultName</parameter>
          <parameter>TermContextChecker/IndexName</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>AnnotationType</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermContextBuilder/AnnotationType</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>ScopeSize</name>
        <type>Integer</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermContextBuilder/ScopeSize</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>SimilarityDistanceClassName</name>
        <value>
          <string>eu.project.ttc.metrics.Jaccard</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>File</name>
        <value>
          <string>ziggurat-alignment.top</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>SourceName</name>
        <value>
          <string>Source</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TargetName</name>
        <value>
          <string>Target</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>ResultName</name>
        <value>
          <string>SourceNormalizedTransferedAligned</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>NormalizedSourceName</name>
        <value>
          <string>SourceNormalized</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>NormalizedTargetName</name>
        <value>
          <string>TargetNormalized</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TransferedSourceName</name>
        <value>
          <string>SourceNormalizedTransfered</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>AssociationRateClassName</name>
        <value>
          <string>eu.project.ttc.metrics.LogLikelihood</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>AnnotationType</name>
        <value>
          <string>eu.project.ttc.types.TermContextEntryAnnotation</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>ScopeSize</name>
        <value>
          <integer>7</integer>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>TermContextBuilder</node>
        <node>TermContextComputer</node>
        <node>SourceTermContextNormalizer</node>
        <node>TargetTermContextNormalizer</node>
        <node>TermContextFilter</node>
        <node>TermContextShrinker</node>
        <node>TermContextTransferer</node>
        <node>TermContextAligner</node>
        <node>TermContextChecker</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.TermContextIndexAnnotation</type>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.TermContextEntryAnnotation</type>
        </inputs>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.TermContextVectorAnnotation</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration>
