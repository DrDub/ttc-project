<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>eu.project.ttc.engines.TermIndexer</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>Term Indexer</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>Feature</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>Language</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>AssociationRateClassName</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>Feature</name>
        <value>
          <string>org.apache.uima.examples.SourceDocumentInformation:lastSegment</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Language</name>
        <value>
          <string>x-unspecified</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>AssociationRateClassName</name>
        <value>
          <string>eu.project.ttc.metrics.LogLikelihood</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import name="eu.project.ttc.types.TermSuiteTypeSystem"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>true</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <externalResourceDependencies>
    <externalResourceDependency>
      <key>SimpleTermFrequency</key>
      <description/>
      <interfaceName>eu.project.ttc.resources.SimpleTermFrequency</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
    <externalResourceDependency>
      <key>ComplexTermFrequency</key>
      <description/>
      <interfaceName>eu.project.ttc.resources.ComplexTermFrequency</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
  </externalResourceDependencies>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>SimpleTermFrequencyResource</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/resources/SimpleTermFrequency.class</fileUrl>
        </fileResourceSpecifier>
        <implementationName>eu.project.ttc.resources.SimpleTermFrequency</implementationName>
      </externalResource>
      <externalResource>
        <name>ComplexTermFrequency</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/resources/ComplexTermFrequency.class</fileUrl>
        </fileResourceSpecifier>
        <implementationName>eu.project.ttc.resources.ComplexTermFrequency</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>SimpleTermFrequency</key>
        <resourceName>SimpleTermFrequencyResource</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>ComplexTermFrequency</key>
        <resourceName>ComplexTermFrequency</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
