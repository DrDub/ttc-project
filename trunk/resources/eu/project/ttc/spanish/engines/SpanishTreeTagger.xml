<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="SpanishWordTokenizer">
      <import name="eu.project.ttc.spanish.engines.SpanishWordTokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="XmiCasConsumer">
      <import name="eu.project.ttc.all.engines.XmiCasConsumer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SpanishTreeTaggerWrapper">
      <import name="eu.project.ttc.spanish.engines.SpanishTreeTaggerWrapper"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="SpanishTreeTaggerCategoryNormalizer">
      <import name="eu.project.ttc.spanish.engines.SpanishTreeTaggerCategoryNormalizer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>Spanish TreeTagger</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>TreeTaggerHomeDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>SpanishTreeTaggerWrapper/TreeTaggerHomeDirectory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Directory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>XmiCasConsumer/Directory</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>TreeTaggerHomeDirectory</name>
        <value>
          <string>/usr/share/tree-tagger</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Directory</name>
        <value>
          <string>/tmp</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>SpanishWordTokenizer</node>
        <node>SpanishTreeTaggerWrapper</node>
        <node>SpanishTreeTaggerCategoryNormalizer</node>
        <node>XmiCasConsumer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>es</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
