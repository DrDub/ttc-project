<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="RussianStemmer">
      <import name="eu.project.ttc.russian.engines.RussianStemmer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianTreeTaggerTenseNormalizer">
      <import name="eu.project.ttc.russian.engines.RussianTreeTaggerTenseNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianTreeTaggerSubcategoryNormalizer">
      <import name="eu.project.ttc.russian.engines.RussianTreeTaggerSubcategoryNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianWordTokenizer">
      <import name="eu.project.ttc.russian.engines.RussianWordTokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianTreeTaggerMoodNormalizer">
      <import name="eu.project.ttc.russian.engines.RussianTreeTaggerMoodNormalizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="XmiCasConsumer">
      <import name="eu.project.ttc.all.engines.XmiCasConsumer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianTreeTaggerWrapper">
      <import name="eu.project.ttc.russian.engines.RussianTreeTaggerWrapper"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianTreeTaggerCategoryNormalizer">
      <import name="eu.project.ttc.russian.engines.RussianTreeTaggerCategoryNormalizer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>Russian TreeTagger Analysis Engine</name>
    <description/>
    <version>1.0</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>TreeTaggerHomeDirectory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>RussianTreeTaggerWrapper/TreeTaggerHomeDirectory</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>Directory</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>XmiCasConsumer/Directory</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>TreeTaggerHomeDirectory</name>
        <value>
          <string>/usr/share/tree-tagger</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Directory</name>
        <value>
          <string>/tmp</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>RussianWordTokenizer</node>
        <node>RussianTreeTaggerWrapper</node>
        <node>RussianTreeTaggerCategoryNormalizer</node>
        <node>RussianTreeTaggerSubcategoryNormalizer</node>
        <node>RussianTreeTaggerMoodNormalizer</node>
        <node>RussianTreeTaggerTenseNormalizer</node>
        <node>RussianStemmer</node>
        <node>XmiCasConsumer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">eu.project.ttc.types.WordAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>ru</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
