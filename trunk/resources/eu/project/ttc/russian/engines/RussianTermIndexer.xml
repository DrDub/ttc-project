<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>fr.free.rocheteau.jerome.engines.Indexer</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>Russian Term Indexer</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>Type</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>Type</name>
        <value>
          <string>eu.project.ttc.types.TermAnnotation</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import name="eu.project.ttc.types.TermSuiteTypeSystem"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <externalResourceDependencies>
    <externalResourceDependency>
      <key>Index</key>
      <description/>
      <interfaceName>fr.free.rocheteau.jerome.models.Index</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
    <externalResourceDependency>
      <key>Listener</key>
      <description/>
      <interfaceName>fr.free.rocheteau.jerome.models.IndexListener</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
    <externalResourceDependency>
      <key>GeneralLanguage</key>
      <description/>
      <interfaceName>eu.project.ttc.models.GeneralLanguage</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
  </externalResourceDependencies>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>DefaultIndex</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/all/engines/IndexWriter.xml</fileUrl>
        </fileResourceSpecifier>
        <implementationName>fr.free.rocheteau.jerome.models.DefaultIndex</implementationName>
      </externalResource>
      <externalResource>
        <name>TermFrequencyListener</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/models/TermFrequencyListener.class</fileUrl>
        </fileResourceSpecifier>
        <implementationName>eu.project.ttc.models.TermFrequencyListener</implementationName>
      </externalResource>
      <externalResource>
        <name>RussianGeneralLanguage</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>eu/project/ttc/russian/resources/GeneralLanguage.Russian</fileUrl>
        </fileResourceSpecifier>
        <implementationName>eu.project.ttc.models.GeneralLanguageResource</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>Index</key>
        <resourceName>DefaultIndex</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>Listener</key>
        <resourceName>TermFrequencyListener</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>GeneralLanguage</key>
        <resourceName>RussianGeneralLanguage</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
