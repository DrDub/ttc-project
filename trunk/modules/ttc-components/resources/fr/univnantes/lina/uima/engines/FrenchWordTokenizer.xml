<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="SimpleWordTokenizer">
      <import name="fr.univnantes.lina.uima.engines.SimpleWordTokenizer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchCompoundWordTokenizer">
      <import name="fr.univnantes.lina.uima.engines.FrenchCompoundWordTokenizer"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>French Word Tokenizer</name>
    <description/>
    <version>11-01-01</version>
    <vendor/>
    <configurationParameters>
      <configurationParameter>
        <name>SofaNames</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>SimpleWordTokenizer/SofaNames</parameter>
          <parameter>FrenchCompoundWordTokenizer/SofaNames</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>SofaNames</name>
        <value>
          <array>
            <string>_InitialView</string>
          </array>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>SimpleWordTokenizer</node>
        <node>FrenchCompoundWordTokenizer</node>
      </fixedFlow>
    </flowConstraints>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">org.apache.uima.TokenAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>fr</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
