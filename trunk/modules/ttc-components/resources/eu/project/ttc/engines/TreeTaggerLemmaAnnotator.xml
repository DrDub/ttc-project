<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="SpanishTreeTaggerLemmaAnnotator">
      <import name="eu.project.ttc.engines.SpanishTreeTaggerLemmaAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="FrenchTreeTaggerLemmaAnnotator">
      <import name="eu.project.ttc.engines.FrenchTreeTaggerLemmaAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="EnglishTreeTaggerLemmaAnnotator">
      <import name="eu.project.ttc.engines.EnglishTreeTaggerLemmaAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="GermanTreeTaggerLemmaAnnotator">
      <import name="eu.project.ttc.engines.GermanTreeTaggerLemmaAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="RussianTreeTaggerLemmaAnnotator">
      <import name="eu.project.ttc.engines.RussianTreeTaggerLemmaAnnotator"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <flowController key="LanguageFlowController">
    <import name="fr.univnantes.lina.uima.engines.LanguageFlowController"/>
  </flowController>
  <analysisEngineMetaData>
    <name>TreeTagger Lemma Annotator</name>
    <description>This Analysis Engine maps the feature 'lemma' of annotations which type is 'org.apache.uima.TreeTaggerAnnotation' into the feature 'value' of annotations which type is 'org.apache.uima.LemmaAnnotation'.

This Aggregate Analysis Engine deals with 5 languages: English, French, German, Spanish and Russian.

Subjects of Analysis to process can be defined by the multi-valued String parameter 'SofaNames' set to the single value '_InitialView' by default.</description>
    <version>11-06-06</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>SofaNames</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>SpanishTreeTaggerLemmaAnnotator/SofaNames</parameter>
          <parameter>FrenchTreeTaggerLemmaAnnotator/SofaNames</parameter>
          <parameter>EnglishTreeTaggerLemmaAnnotator/SofaNames</parameter>
          <parameter>GermanTreeTaggerLemmaAnnotator/SofaNames</parameter>
          <parameter>RussianTreeTaggerLemmaAnnotator/SofaNames</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>SofaNames</name>
        <value>
          <array>
            <string>_InitialView</string>
          </array>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>EnglishTreeTaggerLemmaAnnotator</node>
        <node>FrenchTreeTaggerLemmaAnnotator</node>
        <node>GermanTreeTaggerLemmaAnnotator</node>
        <node>RussianTreeTaggerLemmaAnnotator</node>
        <node>SpanishTreeTaggerLemmaAnnotator</node>
      </fixedFlow>
    </flowConstraints>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type>org.apache.uima.TreeTaggerAnnotation</type>
          <feature>org.apache.uima.TreeTaggerAnnotation:lemma</feature>
        </inputs>
        <outputs>
          <type>org.apache.uima.LemmaAnnotation</type>
          <feature>org.apache.uima.LemmaAnnotation:value</feature>
        </outputs>
        <languagesSupported>
          <language>en</language>
          <language>fr</language>
          <language>de</language>
          <language>ru</language>
          <language>es</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
