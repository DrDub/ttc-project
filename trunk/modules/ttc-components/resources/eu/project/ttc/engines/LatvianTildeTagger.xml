<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>true</primitive>
  <annotatorImplementationName>eu.project.ttc.engines.TildeTagger</annotatorImplementationName>
  <analysisEngineMetaData>
    <name>Latvian Tilde Tagger</name>
    <description>This Analysis Engine launches the Tilde's Tagger script.

This Analysis Engine is devoted to Latvian processing.

Subjects of Analysis to process can be defined by the multi-valued String parameter 'SofaNames' set to the single value '_InitialView' by default.</description>
    <version>11-06-06</version>
    <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
    <configurationParameters>
      <configurationParameter>
        <name>SofaNames</name>
        <type>String</type>
        <multiValued>true</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>TaggerFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>Username</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
      <configurationParameter>
        <name>Password</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>SofaNames</name>
        <value>
          <array>
            <string>_InitialView</string>
          </array>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Username</name>
        <value>
          <string>TTC</string>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>Password</name>
        <value>
          <string>TTCt@g3r</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <typeSystemDescription>
      <imports>
        <import name="fr.univnantes.lina.uima.types.TreeTaggerAnnotation"/>
      </imports>
    </typeSystemDescription>
    <typePriorities/>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">org.apache.uima.TreeTaggerAnnotation</type>
        </outputs>
        <languagesSupported>
          <language>lv</language>
        </languagesSupported>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <externalResourceDependencies>
    <externalResourceDependency>
      <key>Lexicon</key>
      <description/>
      <interfaceName>fr.univnantes.lina.uima.models.Lexicon</interfaceName>
      <optional>false</optional>
    </externalResourceDependency>
  </externalResourceDependencies>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>FrenchLexicon</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>fr/univnantes/lina/uima/resources/lexicons/french.txt</fileUrl>
        </fileResourceSpecifier>
        <implementationName>fr.univnantes.lina.uima.models.Lexicon</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>Lexicon</key>
        <resourceName>FrenchLexicon</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
