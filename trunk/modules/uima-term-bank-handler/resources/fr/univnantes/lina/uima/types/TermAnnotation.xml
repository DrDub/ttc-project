<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>Term Annotation</name>
  <description/>
  <version>1.0</version>
  <vendor>University of Nantes (LINA, UMR CNRS 6241)</vendor>
  <types>
    <typeDescription>
      <name>org.apache.uima.TermAnnotation</name>
      <description/>
      <supertypeName>org.apache.uima.TermComponentAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>document</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>complexity</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>frequency</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>specificity</name>
          <description/>
          <rangeTypeName>uima.cas.Double</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>org.apache.uima.TermComponentAnnotation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>category</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>lemma</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>language</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
        <featureDescription>
          <name>reCoveredText</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>org.apache.uima.SingleWordTermAnnotation</name>
      <description/>
      <supertypeName>org.apache.uima.TermAnnotation</supertypeName>
    </typeDescription>
    <typeDescription>
      <name>org.apache.uima.MultiWordTermAnnotation</name>
      <description/>
      <supertypeName>org.apache.uima.TermAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>components</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>org.apache.uima.TermComponentAnnotation</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>org.apache.uima.CompoundWordTermAnnotation</name>
      <description/>
      <supertypeName>org.apache.uima.TermAnnotation</supertypeName>
      <features>
        <featureDescription>
          <name>components</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>org.apache.uima.TermComponentAnnotation</elementType>
        </featureDescription>
      </features>
    </typeDescription>
    <typeDescription>
      <name>org.apache.uima.TermEntryAnnotation</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>
        <featureDescription>
          <name>occurrences</name>
          <description/>
          <rangeTypeName>uima.cas.FSArray</rangeTypeName>
          <elementType>org.apache.uima.TermAnnotation</elementType>
        </featureDescription>
        <featureDescription>
          <name>reCoveredText</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>
      </features>
    </typeDescription>
  </types>
</typeSystemDescription>
