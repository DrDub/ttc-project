<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="TermBankIndexer">
      <import name="fr.univnantes.lina.uima.engines.TermBankIndexer"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermBankWriter">
      <import name="fr.univnantes.lina.uima.engines.TermBankWriter"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="TermBankFilter">
      <import name="fr.univnantes.lina.uima.engines.TermBankFilter"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>Term Bank Handler</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback">
      <configurationParameter>
        <name>IndexSingleWordTerms</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermBankIndexer/IndexSingleWordTerms</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>IndexMultiWordTerms</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermBankIndexer/IndexMultiWordTerms</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>IndexCompoundWordTerms</name>
        <type>Boolean</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermBankIndexer/IndexCompoundWordTerms</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>DictionaryFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>false</mandatory>
        <overrides>
          <parameter>TermBankFilter/DictionaryFile</parameter>
        </overrides>
      </configurationParameter>
      <configurationParameter>
        <name>TermBankFile</name>
        <type>String</type>
        <multiValued>false</multiValued>
        <mandatory>true</mandatory>
        <overrides>
          <parameter>TermBankWriter/TermBankFile</parameter>
        </overrides>
      </configurationParameter>
    </configurationParameters>
    <configurationParameterSettings>
      <nameValuePair>
        <name>IndexSingleWordTerms</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>IndexMultiWordTerms</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>IndexCompoundWordTerms</name>
        <value>
          <boolean>false</boolean>
        </value>
      </nameValuePair>
      <nameValuePair>
        <name>TermBankFile</name>
        <value>
          <string>term-bank.obj</string>
        </value>
      </nameValuePair>
    </configurationParameterSettings>
    <flowConstraints>
      <fixedFlow>
        <node>TermBankIndexer</node>
        <node>TermBankFilter</node>
        <node>TermBankWriter</node>
      </fixedFlow>
    </flowConstraints>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs>
          <type allAnnotatorFeatures="true">org.apache.uima.TermAnnotation</type>
        </inputs>
        <outputs/>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration>
    <externalResources>
      <externalResource>
        <name>DefaultTermBank</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>fr/univnantes/lina/uima/models/TermBank.class</fileUrl>
        </fileResourceSpecifier>
        <implementationName>fr.univnantes.lina.uima.models.TermBankResource</implementationName>
      </externalResource>
      <externalResource>
        <name>DefaultDictionary</name>
        <description/>
        <fileResourceSpecifier>
          <fileUrl>fr/univnantes/lina/uima/resources/Dictionary.Default</fileUrl>
        </fileResourceSpecifier>
        <implementationName>fr.univnantes.lina.uima.dictionaries.DictionaryResource</implementationName>
      </externalResource>
    </externalResources>
    <externalResourceBindings>
      <externalResourceBinding>
        <key>TermBankIndexer/TermBank</key>
        <resourceName>DefaultTermBank</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermBankWriter/TermBank</key>
        <resourceName>DefaultTermBank</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermBankFilter/TermBank</key>
        <resourceName>DefaultTermBank</resourceName>
      </externalResourceBinding>
      <externalResourceBinding>
        <key>TermBankFilter/Dictionary</key>
        <resourceName>DefaultDictionary</resourceName>
      </externalResourceBinding>
    </externalResourceBindings>
  </resourceManagerConfiguration>
</analysisEngineDescription>
